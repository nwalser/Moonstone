@ChildContent?.Invoke(WorkspaceManager.Workspaces[WorkspaceId])

@code {
    [Inject] public required WorkspaceManager WorkspaceManager { get; set; }

    [Parameter] public int WorkspaceId { get; set; }
    [Parameter] public RenderFragment<Workspace>? ChildContent { get; set; }
    
    
    protected override void OnInitialized()
    {
        WorkspaceManager.Workspaces[WorkspaceId].ExternalChange.Subscribe(_ => InvokeAsync(StateHasChanged));
        base.OnInitialized();
    }
}