@page "/counter"

<h1>Counter</h1>

<p role="status">Current count: @currentCount</p>

<MudButton Variant="Variant.Outlined" class="btn btn-primary" @onclick="IncrementCount">Click me</MudButton>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
